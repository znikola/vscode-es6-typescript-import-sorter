{
    "name": "imports-sorter",
    "displayName": "Imports sorter for ES6 and TypeScript",
    "description": "Sorting and grouping import statements for ES6 and TypeScript",
    "version": "0.0.1",
    "publisher": "nikolazaric",
    "engines": {
        "vscode": "^1.26.0"
    },
    "license": "SEE LICENCE IN LICENCE.md",
    "categories": [
        "Formatters"
    ],
    "keywords": [
        "imports",
        "sort imports",
        "order imports",
        "group imports",
        "import statements",
        "import clauses",
        "TypeScript",
        "ES6"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sortImports",
                "title": "Sort imports"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42",
        "prettier": "1.14.2"
    }
}
